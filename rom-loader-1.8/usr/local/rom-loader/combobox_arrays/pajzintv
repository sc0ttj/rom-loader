# VOICE combobox
VOICE_ARRAY=(Default true false)
VOICE_ITEMS="<item>$VOICE</item>" #stored value should be first in list
for i in "${VOICE_ARRAY[@]}"
do
	if [ "$i" != "$VOICE" ];then
		VOICE_ITEMS=`echo "$VOICE_ITEMS<item>$i</item>"`
	fi
done

# SIZE combobox
SIZE_ARRAY=(Default 320x200 640x480)
SIZE_ITEMS="<item>$SIZE</item>" #stored value should be first in list
for i in "${SIZE_ARRAY[@]}"
do
	if [ "$i" != "$SIZE" ];then
		SIZE_ITEMS=`echo "$SIZE_ITEMS<item>$i</item>"`
	fi
done

