# AUDIO combobox
AUDIO_ARRAY=(Default nosound 11025 22050 44100)
AUDIO_ITEMS="<item>$AUDIO</item>" #stored value should be first in list
for i in "${AUDIO_ARRAY[@]}"
do
	if [ "$i" != "$AUDIO" ];then
		AUDIO_ITEMS=`echo "$AUDIO_ITEMS<item>$i</item>"`
	fi
done

# AUDIOFILE combobox
AUDIOFILE_ARRAY=(Disabled intv-01.wav intv-02.wav intv-03.wav intv-04.wav intv-05.wav intv-06.wav intv-07.wav intv-0008.wav intv-09.wav)
AUDIOFILE_ITEMS="<item>$AUDIOFILE</item>" #stored value should be first in list
for i in "${AUDIOFILE_ARRAY[@]}"
do
	if [ "$i" != "$AUDIOFILE" ];then
		AUDIOFILE_ITEMS=`echo "$AUDIOFILE_ITEMS<item>$i</item>"`
	fi
done

