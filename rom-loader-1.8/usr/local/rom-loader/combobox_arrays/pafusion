SMSBIOS_ARRAY=(Default true false)
SMSBIOS_ITEMS="<item>$SMSBIOS</item>" #stored value should be first in list
for i in "${SMSBIOS_ARRAY[@]}"
do
	if [ "$i" != "$SMSBIOS" ];then
		SMSBIOS_ITEMS=`echo "$SMSBIOS_ITEMS<item>$i</item>"`
	fi
done

