NGSYSTEM_ARRAY=(Default home arcade unibios)
NGSYSTEM_ITEMS="<item>$NGSYSTEM</item>" #stored value should be first in list
for i in "${NGSYSTEM_ARRAY[@]}"
do
	if [ "$i" != "$NGSYSTEM" ];then # if not already the selected value
		NGSYSTEM_ITEMS=`echo "$NGSYSTEM_ITEMS<item>$i</item>"` # add the value to the list
	fi
done

