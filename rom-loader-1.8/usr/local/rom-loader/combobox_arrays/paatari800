ATARI_SYSTEM_ARRAY=(Default blank atari 1200 xl xe 320xe rambo xegs 5200 1400 xld)
ATARI_SYSTEM_ITEMS="<item>$ATARI_SYSTEM</item>" #stored value should be first in list
for i in "${ATARI_SYSTEM_ARRAY[@]}"
do
	if [ "$i" != "$ATARI_SYSTEM" ];then # if not already the selected value
		ATARI_SYSTEM_ITEMS=`echo "$ATARI_SYSTEM_ITEMS<item>$i</item>"` # add the value to the list
	fi
done

